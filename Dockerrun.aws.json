{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [
		{
			"name": "client",
			"image": "jongober/multi-client",
			"hostname": "client",
			"essential": false
		},
		{
			"name": "expressapi",
			"image": "jongober/multi-server",
			"hostname": "expressapi",
			"essential": false
		},
		{
			"name": "worker",
			"image": "jongober/multi-worker",
			"essential": false
		},
		{
			"name": "nginx-router",
			"image": "jongober/multi-nginx",
			"essential": true,
			"portMappings": [
				{
					"hostPort": 80,
					"containerPort": 80
				}
			],
			"links": ["client", "expressapi"]
		}		
	]
}